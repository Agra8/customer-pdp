<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_pdp_master_fields_list" model="ir.ui.view">
        <field name="name">pdp.master.fields.list</field>
        <field name="model">pdp.master.fields</field>
        <field name="arch" type="xml">
            <list string="Master Fields" editable="bottom">
                <field name="field_id" required ="1" options="{'no_create': True, 'no_open': True}"/>
                <field name="field_name" readonly="1"/>
                <field name="field_type" readonly="1"/>
                <!-- <field name="field_name"/> -->
            </list>
        </field>
    </record>
    
    <record id="action_pdp_master_fields" model="ir.actions.act_window">
        <field name="name"> PDP Master Fields</field>
        <field name="res_model">pdp.master.fields</field>
        <field name="view_mode">list</field>
    </record>

</odoo>